{
  "name": "binance-price-recorder",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/binance-price-recorder/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/binance-price-recorder",
        "main": "apps/binance-price-recorder/src/main.ts",
        "tsConfig": "apps/binance-price-recorder/tsconfig.app.json",
        "generatePackageJson": true,
        "webpackConfig": "apps/binance-price-recorder/webpack.config.js"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "binance-price-recorder:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "binance-price-recorder:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/binance-price-recorder/**/*.ts"],
        "fix": true
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/binance-price-recorder/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "build-docker-image": {
      "executor": "@nx-tools/nx-container:build",
      "options": {
        "engine": "docker",
        "metadata": {
          "images": ["narwhal-project-ts/binance-price-recorder"],
          "load": true,
          "tags": ["latest"]
        }
      }
    },
    "export-docker-image": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx build",
          "nx build-docker-image",
          "mkdir -p deploy",
          "rm -f deploy/binance-price-recorder:latest",
          "docker save narwhal-project-ts/binance-price-recorder > deploy/binance-price-recorder_latest.tar"
        ],
        "parallel": false
      }
    }
  },
  "tags": []
}
